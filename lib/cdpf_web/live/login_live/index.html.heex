<h1>Login</h1>
<%= if @error do %>
  <p class="error"><%= @error %></p>
<% end %>
<form phx-submit="submit">
  <label>Email:</label>
  <input type="email" name="email" value="" required />
  <label>Password:</label>
  <input type="password" name="password" value="" required />
  <button type="submit">Login</button>
</form>
